{
    "overrides": [
        {
            "files": "*.css",
            "options": {
                "singleQuote": false
            }
        }
    ],
    "arrowParens": "avoid",
    "plugins": ["prettier-plugin-svelte"],
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "svelteSortOrder": "options-scripts-markup-styles",
    "svelteStrictMode": false,
    "svelteBracketNewLine": true,
    "svelteIndentScriptAndStyle": true,
    "tabWidth": 4,
    "trailingComma": "all"
}
